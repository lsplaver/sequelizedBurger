<div class="newBody">
    <h1>Eat-Da-Burger</h1>
    <div>
        <div class="uneaten">
            <ul>
                <label>Uneaten Burgers</label>
                {{#each burgers}} {{#unless this.devoured}}
                <!-- || devoured || this.burger.devoured || this.data.devoured}} -->
                <li>
                <li>
                    ID: {{ id }} BurgerName: {{ burger_name }} Devoured: {{ devoured }}
                </li>
                <li>
                    ID: {{ this.id }} BurgerName: {{ this.burger_name }} Devoured: {{ this.devoured }}
                </li>
                <li>
                    ID: {{ this.data.id }} BurgerName: {{ this.data.burger_name }} Devoured: {{ this.data.devoured }}
                </li>
                <li>
                    ID: {{ this.burger.id }} BurgerName: {{ this.burger.burger_name }} Devoured: {{ this.burger.devoured
                    }}
                </li>
                <button class="isEaten" data-id="{{this.id}}" data-waseaten="true">Devour It!</button>
                </li>
                {{/unless}} {{/each}}
            </ul>
        </div>
        <div class="eaten">
            <ul>
                <label>Already Eaten Burgers</label>
                {{#each burgers}} {{#if this.devoured}}
                <li>
                    ID: {{ this.data.id }} BurgerName: {{ this.data.burger_name }} Devoured: {{ this.data.devoured }}
                    ID: {{ this.burger.id }} BurgerName: {{ this.burger.burger_name }} Devoured: {{ this.bruger.devoured
                    }}
                    ID: {{ this.id }} BurgerName: {{ this.burger_name }} Devoured: {{ this.devoured }}
                    ID: {{ id }} BurgerName {{ burger_name }} Devoured: {{ devoured }}
                </li>
                {{/if}} {{/each}}
            </ul>
        </div>
        <div class="newBurger">
            <form class="newForm">
                <br>
                <div>
                    <label for="inputBurger">Enter the name of your burger here:</label>
                    <input type="text" id="inputBurger" name="burgerName">
                </div>
                <br>
                <div>
                    <label for="btnSubmit">Submit your order here:</label>
                    <button id="btnSubmit" type="submit">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>